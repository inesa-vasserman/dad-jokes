#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/inesavasserman/cdemos/js/dad-jokes/node_modules/.pnpm/stylelint-config-prettier@9.0.5_stylelint@16.25.0_typescript@5.9.3_/node_modules/stylelint-config-prettier/bin/node_modules:/Users/inesavasserman/cdemos/js/dad-jokes/node_modules/.pnpm/stylelint-config-prettier@9.0.5_stylelint@16.25.0_typescript@5.9.3_/node_modules/stylelint-config-prettier/node_modules:/Users/inesavasserman/cdemos/js/dad-jokes/node_modules/.pnpm/stylelint-config-prettier@9.0.5_stylelint@16.25.0_typescript@5.9.3_/node_modules:/Users/inesavasserman/cdemos/js/dad-jokes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/inesavasserman/cdemos/js/dad-jokes/node_modules/.pnpm/stylelint-config-prettier@9.0.5_stylelint@16.25.0_typescript@5.9.3_/node_modules/stylelint-config-prettier/bin/node_modules:/Users/inesavasserman/cdemos/js/dad-jokes/node_modules/.pnpm/stylelint-config-prettier@9.0.5_stylelint@16.25.0_typescript@5.9.3_/node_modules/stylelint-config-prettier/node_modules:/Users/inesavasserman/cdemos/js/dad-jokes/node_modules/.pnpm/stylelint-config-prettier@9.0.5_stylelint@16.25.0_typescript@5.9.3_/node_modules:/Users/inesavasserman/cdemos/js/dad-jokes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/stylelint-config-prettier@9.0.5_stylelint@16.25.0_typescript@5.9.3_/node_modules/stylelint-config-prettier/bin/check.js" "$@"
else
  exec node  "$basedir/../.pnpm/stylelint-config-prettier@9.0.5_stylelint@16.25.0_typescript@5.9.3_/node_modules/stylelint-config-prettier/bin/check.js" "$@"
fi
